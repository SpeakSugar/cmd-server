{
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testRegex": "^.+\\.test\\.ts$",
  "moduleDirectories": [
    "node_modules"
  ],
  "moduleFileExtensions": [
    "ts",
    "js",
    "json"
  ],
  "testEnvironment": "node",
  "reporters": [
    "default",
    [
      "jest-html-reporters",
      {
        "publicPath": "./test-results/html"
      }
    ]
  ]
}
